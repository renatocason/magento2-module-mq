{
    "name": "room40coding/magento2-module-mq",
    "description": "Magento 2 Message Queue OS Module by renatocason",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "magento/framework": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5.13",
        "squizlabs/php_codesniffer": "3.3.1",
        "phpmd/phpmd": "@stable",
        "php-coveralls/php-coveralls": "~2.1.0"
    },
    "conflict": {
        "renatocason/magento2-module-mq": "*"
    },
    "scripts": {
        "test": "phpunit"
    },
    "type": "magento2-module",
    "license": [
        "OSL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Rcason\\Mq\\": ""
        }
    }
}
