{
    "name": "renatocason/magento2-module-mq",
    "description": "Magento 2 Message Queue OS Module",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/augustash/magento2-module-mq-ampq.git"
        },
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "php": "~7.0.0",
        "symfony/console": "~2.3 <2.7",
        "magento/framework": "101.0.*",
        "magento/module-store": "100.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.0",
        "squizlabs/php_codesniffer": "1.5.3",
        "phpmd/phpmd": "@stable",
        "satooshi/php-coveralls": "~1.0.1"
    },
    "suggest": {
        "renatocason/magento2-module-mq-mysql": "~0.1.0",
        "renatocason/magento2-module-mq-amqp": "dev-master"
    },
    "type": "magento2-module",
    "license": [
        "OSL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Rcason\\Mq\\": ""
        }
    }
}
